<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<Setting note="软件设置">
  <TextArea note="文本域的相关设置">
    <isLineWrap note="是否自动换行">true</isLineWrap>
    <isWrapStyleWord note="是否单词边界换行">true</isWrapStyleWord>
    <autoIndent note="是否可自动缩进">false</autoIndent>
    <tabReplaceBySpace note="是否以空格代替Tab键">false</tabReplaceBySpace>
    <tabSize note="Tab键所占字符数">4</tabSize>
    <autoComplete note="是否自动完成">false</autoComplete>
    <defaultCharEncoding note="默认字符编码格式">GB18030</defaultCharEncoding>
    <defaultLineSeparator note="默认换行符格式">跟随系统</defaultLineSeparator>
    <font name="宋体" size="14" style="0" note="文本域字体"/>
    <colorStyle fontColor="" backColor="" caretColor="" selFontColor="" selBackColor="" bracketBackColor="" lineBackColor="" wordBackColor="" markBackColor="" bookmarkColor="" note="配色方案：字体颜色，背景颜色，光标颜色，选区字体颜色，选区背景颜色，匹配括号背景颜色，当前行背景颜色，匹配文本背景颜色，标记文本背景颜色，书签颜色"/>
  </TextArea>
  <FindReplace note="查找/替换的相关设置">
    <matchCase note="是否区分大小写">true</matchCase>
    <matchWholeWord note="是否全词匹配">false</matchWholeWord>
    <isWrap note="是否循环查找">true</isWrap>
    <findDown note="是否向下查找">true</findDown>
    <searchStyle note="搜索模式">0</searchStyle>
    <addBookmark note="是否在标记时添加书签">false</addBookmark>
    <clearBookmark note="是否在标记时清除已有书签">false</clearBookmark>
    <clearMark note="是否在标记时清除已有标记">false</clearMark>
  </FindReplace>
  <View note="显示的相关设置">
    <viewToolBar note="是否显示工具栏">true</viewToolBar>
    <viewStateBar note="是否显示状态栏">true</viewStateBar>
    <viewFileTree note="是否显示文件树">false</viewFileTree>
    <viewLineNumber note="是否显示行号栏">false</viewLineNumber>
    <viewSearchResult note="是否显示查找结果面板">false</viewSearchResult>
    <viewAlwaysOnTop note="是否前端显示">false</viewAlwaysOnTop>
    <viewLockResizable note="是否锁定窗口">false</viewLockResizable>
    <viewTabPolicy note="是否显示多行标签">true</viewTabPolicy>
    <viewClickToClose note="是否双击关闭标签">true</viewClickToClose>
    <viewTabIcon note="是否显示指示图标">true</viewTabIcon>
    <viewLookAndFeel note="当前外观的索引号">-1</viewLookAndFeel>
    <viewFrameSize width="600" height="500" note="窗口大小"/>
  </View>
  <Files note="所有已打开的文件">
    <file/>
  </Files>
  <Shortcuts note="所有自定义快捷键的设置">
    <shortcut/>
  </Shortcuts>
</Setting>
